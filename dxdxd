local function EWHXKU_fake_script() -- All.BLScript 
	local script = Instance.new('LocalScript', All)

	local BlButton = script.Parent.Parent.menu.BLBu
	local BlText = script.Parent.Parent.menu.BLBu.TextLabel
	
	local BlOn = 0
	
	BlButton.MouseButton1Down:connect(function()
		if BlText.TextColor3 == Color3.fromRGB(85, 255, 127) then
			
			BlText.TextColor3 = Color3.fromRGB(255, 0, 0)
			BlText.Text = "BLACKLIST: OFF"
			BlOn = 0
		else
			if BlText.TextColor3 == Color3.fromRGB(255, 0, 0) then
				
				BlText.TextColor3 = Color3.fromRGB(85, 255, 127)
				BlText.Text = "BLACKLIST: ON"
				BlOn = 1
				
			end
			
		end
	end)
	
	
	
end
coroutine.wrap(EWHXKU_fake_script)()
local function VOPBDBZ_fake_script() -- All.StartingGUi 
	local script = Instance.new('LocalScript', All)

	
	
	wait(0.5)
	--FUNCTION
	
	local function typewrite(object,text)
		for i = 1,#text,1 do
			object.Text = string.sub(text,1,i)
			wait(0.01)
		end
	end
	
	--LOCALS
	
	local GUI = script.Parent
	local Menu = script.Parent.Parent.menu
	local Menus = script.Parent.Parent.menu.menus
	
	local TittleType = script.Parent.Parent.menu.Tittle
	
	
	-- GETTING PLAYER DATA
	
	local playerg = game.Players.LocalPlayer
	local userId = playerg.UserId
	local GuiN = script.Parent.Parent.Guix.GuiN
	
	Menu.PlayerName.Text = playerg.Name
	
	GuiN.GTitle.PlayerName.Text = playerg.Name
	
	-- ADMINS
	local colorfav = script.Parent.Parent.Colorfav
	
	if Menu.PlayerName.Text == "ziiGhxst" then
		colorfav.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
	else
		if Menu.PlayerName.Text == "02Vxid" then
			colorfav.BackgroundColor3 = Color3.fromRGB(85, 255, 255)
		else
			if Menu.PlayerName.Text == "HenryRenacuajo" then
				colorfav.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
			end
		end
	
	end
	
	wait(0.02)
	
	GUI.Visible = true
	Menu.Visible = true
	
	-- starting pos: {0.275, 0},{0.166, 0}
	-- final pos: {0.275, 0},{0.321, 0}
	
	
	GUI:TweenPosition(UDim2.new(0.358, 0, 0.321, 0))
	Menu:TweenPosition(UDim2.new(0.358, 0, 0.321, 0))
	local guiT = 1
	local guiM = 1
	
	while guiT > 0  do
		guiT = guiT - 0.05
		GUI.BackgroundTransparency = guiT
		Menus.BackgroundTransparency = guiT
		wait(0.01)
	end
	
	
	
	
	typewrite(TittleType, "BIENVENIDO")
	
	
	
	
	
end
coroutine.wrap(VOPBDBZ_fake_script)()
local function JEGVGR_fake_script() -- All.EndingGui 
	local script = Instance.new('LocalScript', All)

	local InicButton = script.Parent.Parent.menu.INICIAR
	local welcome = script.Parent
	local menu = script.Parent.Parent.menu
	local tittle = script.Parent.Parent.menu.Tittle
	local VerBl = script.Parent.Parent.menu.BLBu.TextLabel
	local bl = script.Parent.Parent.Guix.GuiN.GuiM.BLACKLIST
	local GuiN = script.Parent.Parent.Guix.GuiN
	local colorfav = script.Parent.Parent.Colorfav
	
	local function typewrite(object,text)
		for i = 1,#text,1 do
			object.Text = string.sub(text,1,i)
			wait(0.01)
		end
	end
	
	InicButton.MouseButton1Down:Connect(function()
		script.Disabled = true
		if VerBl.TextColor3 == Color3.fromRGB(255, 0, 0) then
			
			VerBl.TextColor3 = Color3.fromRGB(255, 0, 0)
			bl.TextColor3 = VerBl.TextColor3
			bl.Text = "BLACKLIST: OFF"
			
		else
			
			if VerBl.TextColor3 == Color3.fromRGB(85, 255, 127) then
				VerBl.TextColor3 = Color3.fromRGB(85, 255, 127)
				bl.TextColor3 = VerBl.TextColor3
				bl.Text = "BLACKLIST: ON"
				
			end
		end
		
		typewrite(tittle, "DIVIERTETE")
		wait(0.5)
		menu.Tittle.TextColor3 = colorfav.BackgroundColor3
		wait(0.1)
		menu.Tittle.TextColor3 = Color3.fromRGB(255, 255, 255)
		wait(0.1)
		menu.Tittle.TextColor3 = colorfav.BackgroundColor3
		wait(0.1)
		menu.Tittle.TextColor3 = Color3.fromRGB(255, 255, 255)
		wait(0.1)
		menu.Tittle.TextColor3 = colorfav.BackgroundColor3
		wait(0.1)
		menu.Tittle.TextColor3 = Color3.fromRGB(255, 255, 255)
		wait(1)
		
		welcome:TweenPosition(
			UDim2.new(0.358, 0, -0.450, 0),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quad,
			0.4
		)
		menu:TweenPosition(
			UDim2.new(0.358, 0, -0.450, 0),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quad,
			0.4
		)
		
		wait(1)
		welcome.Visible = false
		menu.Visible = false
		-- first position: {0.015, 0},{0.543, 0}
		-- final position: {0.015, 0},{0.601, 0}
		wait(0.1)
		GuiN.Visible = true
		GuiN.Parent.Active = true
		GuiN.Parent.Draggable = true
		
		GuiN:TweenPosition(
			UDim2.new(0.015, 0, 0.02, 0),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quad,
			1,
			true
		)
		
		-- Guin first pos: {-0.262, 0},{1.437, 0}
		-- Gui final pos: {-0.262, 0},{0.427, 0}
		
		
		-- GUIX org pos: {-0, 0},{0.636, 0}
	
	end)
end
coroutine.wrap(JEGVGR_fake_script)()
