coroutine.wrap(HCZEE_fake_script)()
local function LTHP_fake_script() -- GuiN.Close/Open 
	local script = Instance.new('LocalScript', GuiN)

	-- Guin first pos: {-0.262, 0},{1.437, 0}
	-- Gui final pos: {-0.262, 0},{0.427, 0}
	
	-- ORIGINAL POS
	
	local GuiN = script.Parent
	local Menu = script.Parent.Parent.Parent.All
	local ALL = script.Parent.Parent.Parent.menu
	
	local uis = game:GetService("UserInputService")
	local TfG = 0
	local TfGButton = 0.65
	local Doit = 0
	local Doit2 = 0
	
	uis.InputBegan:Connect(function(Input)
		if Input.KeyCode == Enum.KeyCode.Q and GuiN.Visible == true and Menu.Visible == false then
			script.Disabled = true
			
			GuiN:TweenPosition(
				UDim2.new(0.015, 0, 4.900, 0),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quad,
				0.7,
				true
			)
			
			wait(1)
			GuiN.Visible = false
			wait(0.2)
			
			script.Disabled = false	
	
			
		else
			if Input.KeyCode == Enum.KeyCode.Q and GuiN.Visible == false and Menu.Visible == false then
				script.Disabled = true
				
				GuiN:TweenPosition(
					UDim2.new(0.015, 0, 0.02, 0),
					Enum.EasingDirection.InOut,
					Enum.EasingStyle.Quad,
					0.7,
					true
				)
				
				wait(0.1)
				GuiN.Visible = true
				
				wait(0.2)
				
				script.Disabled = false
			end
		end
	end)
end
coroutine.wrap(LTHP_fake_script)()
local function DGNC_fake_script() -- GuiN.Blacklist 
	local script = Instance.new('LocalScript', GuiN)

	local colorfav = script.Parent.Parent.Parent.Colorfav
	
	local function chatnotify(text)
		game.StarterGui:SetCore("ChatMakeSystemMessage", {Text = text; Color = colorfav.BackgroundColor3; Font = Enum.Font.SourceSans; FontSize = Enum.FontSize.Size42})
	end
	
	
	local BlOGui = script.Parent.GuiM.BLACKLIST
	local BlOn = script.Parent.Parent.Parent.menu.BLBu.TextLabel
	
	local BlOnx = 0
	local function refreshbl()
		if BlOn.TextColor3 == Color3.fromRGB(85, 255, 127) then
			BlOGui.TextColor3 = Color3.fromRGB(85, 255, 127)
			BlOGui.Text = "BLACKLIST: ON"
	
			BlOnx = 0
	
		else
			if BlOn.TextColor3 == Color3.fromRGB(255, 0, 0) then
				BlOGui.TextColor3 = Color3.fromRGB(255, 0, 0)
				BlOGui.Text = "BLACKLIST: OFF"
	
				BlOnx = 1
			end
		end
	end
	
	
	BlOGui.MouseButton1Down:Connect(function()
		if BlOn.TextColor3 == Color3.fromRGB(85, 255, 127) then
			BlOn.TextColor3 = Color3.fromRGB(255, 0, 0)
			
			refreshbl()
		else
			if BlOn.TextColor3 == Color3.fromRGB(255, 0, 0) then
				BlOn.TextColor3 = Color3.fromRGB(85, 255, 127)
				
				refreshbl()
			end
		end
	end)
end
coroutine.wrap(DGNC_fake_script)()
local function QFYQSEO_fake_script() -- Page4.CountFunc 
	local script = Instance.new('LocalScript', Page4)

	function chat(message)
		local args = {
			[1] = message,
			[2] = "All"
		}
	
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end
	
	local Counting = 0
	
	script.Parent.Sub1p1.Count.MouseButton1Down:Connect(function()
		if Counting == 0 then
			script.Disabled = true
			Counting = 1
	
			chat("III")
			wait(1)
			chat("II")
			wait(1)
			chat("I")
			wait(1)
			chat("GO")
			wait(0.5)
	
			Counting = 0
			script.Disabled = false
		end
	end)
	
	
end
coroutine.wrap(QFYQSEO_fake_script)()
local function JBUC_fake_script() -- Page4.Sub1Page 
	local script = Instance.new('LocalScript', Page4)

	local Page = 1
	
	local page1 = script.Parent.Sub1p1
	local page2 = script.Parent.Sub1p2
	local page3 = script.Parent.Sub1p3
	local page4 = script.Parent.Sub1p4
	
	local pagetext = script.Parent.PageNumber
	
	local izq = script.Parent.Izq
	local der = script.Parent.Der
	
	local function refreshP()
		if Page == 1 then
			page1.Visible = true
			page2.Visible = false
			page3.Visible = false
			page4.Visible = false
			
			pagetext.Text = "1/4"
		else
			if Page == 2 then
				page1.Visible = false
				page2.Visible = true
				page3.Visible = false
				page4.Visible = false
				
				pagetext.Text = "2/4"
			else
				if Page == 3 then
					page1.Visible = false
					page2.Visible = false
					page3.Visible = true
					page4.Visible = false
					
					pagetext.Text = "3/4"
				else
					if Page == 4 then
						page1.Visible = false
						page2.Visible = false
						page3.Visible = false
						page4.Visible = true
						
						pagetext.Text = "4/4"
					end
				end
			end
		end
	end
	
	wait(1)
	
	refreshP()
	
	izq.MouseButton1Down:Connect(function()
		Page = Page - 1 
		if Page < 1 then
			Page = 1
		end
		
		refreshP()
	end)
	
	der.MouseButton1Down:Connect(function()
		Page = 	Page + 1
		if Page > 4 then
			Page = 4
		end
		
		refreshP()
	end)
end
coroutine.wrap(JBUC_fake_script)()
local function ILPNGS_fake_script() -- Sub1p2.GunColorT 
	local script = Instance.new('LocalScript', Sub1p2)

	guncolorOn = script.Parent.SETTIME
	
	guncolorOn.MouseButton1Down:Connect(function()
		if guncolorOn.TextColor3 == Color3.fromRGB(255, 0, 0) then
			guncolorOn.TextColor3 = Color3.fromRGB(85, 255, 127)
			guncolorOn.Text = "GUNCOLOR: ON"
			
			
		else
			if guncolorOn.TextColor3 == Color3.fromRGB(85, 255, 127) then
				guncolorOn.TextColor3 = Color3.fromRGB(255, 0, 0)
				guncolorOn.Text = "GUNCOLOR: OFF"
				
				
			end
		end
	end)
end
