ANTISH.MouseButton1Down:connect(function()
	if ANTISH.TextColor3 == Color3.fromRGB(255,0,0) then
		ANTISH.TextColor3 = Color3.fromRGB(85, 255, 127)
		ANTISH.Text = "AUTORESPAWN: ON"

		chatnotify("SE HA ACTIVADO EL AUTORESPAWN.")

		local char = workspace:FindFirstChild(UserName)

		char.Humanoid.Died:Connect(humanoidDied)
		
		
	else
		if ANTISH.TextColor3 == Color3.fromRGB(85, 255, 127) then
			ANTISH.TextColor3 = Color3.fromRGB(255,0,0)
			ANTISH.Text = "AUTORESPAWN: OFF"

			chatnotify("SE HA DESACTIVADO EL AUTORESPAWN.")
			respawns()
		end	
	end
end)

COPLOOP.MouseButton1Down:connect(function()
	if COPLOOP.TextColor3 == Color3.fromRGB(255,0,0) then
		COPLOOP.TextColor3 = Color3.fromRGB(85, 255, 127)
		COPLOOP.Text = "COPLOOP: ON"

		while true do
			wait(.01)
			workspace.Remote.TeamEvent:FireServer("Bright blue")
			if COPLOOP.TextColor3 == Color3.fromRGB(255,0,0) then
				break
			end
		end
	else
		if COPLOOP.TextColor3 == Color3.fromRGB(85, 255, 127) then
			COPLOOP.TextColor3 = Color3.fromRGB(255,0,0)
			COPLOOP.Text = "COPLOOP: OFF"
		end	
	end
end)

