local function UMBFPIC_fake_script() -- Page4.Sub1Fade 
	local script = Instance.new('LocalScript', Page4)

	local izq = script.Parent.Izq
	local der = script.Parent.Der
	
	local fadePage = script.Parent.FadeSub1
	
	local function refreshFade()
		script.Disabled = true
		fadePage.BackgroundTransparency = 0
		while fadePage.BackgroundTransparency < 1 do
			wait(0.001)
			fadePage.BackgroundTransparency = fadePage.BackgroundTransparency + 0.2
	
			if fadePage.BackgroundTransparency == 0.98 then
				script.Disabled = false
			end
		end
	end
	
	wait(0.5)
	
	refreshFade()
	
	izq.MouseButton1Down:Connect(function()
	
		refreshFade()
	end)
	
	der.MouseButton1Down:Connect(function()
	
		refreshFade()
	end)
end
