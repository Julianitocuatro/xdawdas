local function SBUHX_fake_script() -- GuiMX.PageScript 
	local script = Instance.new('LocalScript', GuiMX)

	local Page = 1
	
	local PageN = script.Parent.Parent.GTitle.PageName
	
	local ANTIABUSERS = script.Parent.ANTIABUSERS
	local colorfav = script.Parent.Parent.Parent.Parent.Colorfav
	
	local CONTROLES = script.Parent.CONTROLES
	local LOADSCRIPT = script.Parent.LOADSCRIPT
	local OTROS = script.Parent.OTROS
	
	local movingB = script.Parent.ColorfavB
	
	local page1 = script.Parent.Parent.GuiM
	local page2 = script.Parent.Parent.Page2
	local page3 = script.Parent.Parent.Page3
	local page4 = script.Parent.Parent.Page4
	
	-- 63, 63, 63
	wait(0.03)
	
	local function refreshPage()
		if Page == 1 then
			
			movingB.BackgroundColor3 = colorfav.BackgroundColor3
			movingB:TweenPosition(
				UDim2.new(0.111, 0, 0.051, 0),
				Enum.EasingDirection.InOut,
				Enum.EasingStyle.Quad,
				0.15,
				true
			)
			
			page1.Visible = true
			page2.Visible = false
			page3.Visible = false
			page4.Visible = false
			
			PageN.Text = "CARGAR SCRIPTS"
			
		else
			if Page == 2 then
				
				movingB.BackgroundColor3 = colorfav.BackgroundColor3
				movingB:TweenPosition(
					UDim2.new(0.111, 0, 0.174, 0),
					Enum.EasingDirection.InOut,
					Enum.EasingStyle.Quad,
					0.15,
					true
				)
				
				page1.Visible = false
				page2.Visible = true
				page3.Visible = false
				page4.Visible = false
				
				PageN.Text = "ANTIABUSERS"
			else
				if Page == 3 then
					
					movingB.BackgroundColor3 = colorfav.BackgroundColor3
					movingB:TweenPosition(
						UDim2.new(0.111, 0, 0.701, 0),
						Enum.EasingDirection.InOut,
						Enum.EasingStyle.Quad,
						0.15,
						true
					)
					
					page1.Visible = false
					page2.Visible = false
					page3.Visible = true
					page4.Visible = false
					
					PageN.Text = "CONTROLES"
				else
					if Page == 4 then
						
						movingB.BackgroundColor3 = colorfav.BackgroundColor3
						movingB:TweenPosition(
							UDim2.new(0.111, 0, 0.828, 0),
							Enum.EasingDirection.InOut,
							Enum.EasingStyle.Quad,
							0.15,
							true
						)
						
						page1.Visible = false
						page2.Visible = false
						page3.Visible = false
						page4.Visible = true
						
						PageN.Text = "OTROS"
					end
				end
			end
		end
	end
	
	
	
	wait(1)
	
	refreshPage()
	
	LOADSCRIPT.MouseButton1Down:Connect(function()
		Page = 1
		refreshPage()
	end)
	
	ANTIABUSERS.MouseButton1Down:Connect(function()
		Page = 2
		refreshPage()
	end)
	
	CONTROLES.MouseButton1Down:Connect(function()
		Page = 3
		refreshPage()
	end)
	
	OTROS.MouseButton1Down:Connect(function()
		Page = 4
		refreshPage()
	end)
end
